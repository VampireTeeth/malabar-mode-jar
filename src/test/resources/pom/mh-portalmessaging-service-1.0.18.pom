<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.ihc.framework</groupId>
		<artifactId>java5-pom</artifactId>
		<version>1.0.1</version>
	</parent>

	<groupId>org.ihc.service</groupId>
	<artifactId>mh-portalmessaging-service</artifactId>
	<version>1.0.18</version>
	<packaging>jar</packaging>
	<name>mh-portalmessaging-service</name>
	<description>mh-portalmessaging-service</description>
	<url>http://maven.co.ihc.com/mh-portalmessaging-service</url>
	<ciManagement>
		<system>Bamboo</system>
		<url>http://maven.co.ihc.com/bamboo/browse/MH-PORTALMESSAGINGSERVICE
		</url>
	</ciManagement>
	<inceptionYear>2010</inceptionYear>
	<scm>
		<url>http://subversion.co.ihc.com/svn/webdev/services/MyHealth/modules/mh-portalmessaging-service/trunk</url>
		<connection>scm:svn:http://subversion.co.ihc.com/svn/webdev/services/MyHealth/modules/mh-portalmessaging-service/trunk</connection>
		<developerConnection>scm:svn:http://subversion.co.ihc.com/svn/webdev/services/MyHealth/modules/mh-portalmessaging-service/trunk</developerConnection>
	</scm>
	<developers>
		<developer>
			<id>lpmsmith</id>
			<name>Matthew Smith</name>
			<email>Matthew.Smith@imail.org</email>
			<url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lpmsmith
			</url>
			<organization>Enterprise Web Development</organization>
			<organizationUrl>https://teamspace.intermountain.net/ewd
			</organizationUrl>
			<timezone>MST</timezone>
		</developer>
	</developers>
	<dependencies>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-core</artifactId>
			<version>3.0.5.RELEASE</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>com.ihc</groupId>
			<artifactId>portalmessaging</artifactId>
			<version>1.5.8</version>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>com.ihc</groupId>
			<artifactId>common-ihc</artifactId>
			<version>1.7</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>commons-lang</groupId>
			<artifactId>commons-lang</artifactId>
			<version>2.5</version>
			<type>jar</type>
		</dependency>
		<dependency>
			<groupId>ojdbc</groupId>
			<artifactId>ojdbc</artifactId>
			<version>14</version>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.2</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.beanrunner</groupId>
			<artifactId>beanrunner</artifactId>
			<version>0.0.4</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>org.ihc.audit</groupId>
			<artifactId>AuditEJB</artifactId>
			<version>3.2.0</version>
			<classifier>glassfishclient</classifier>
		</dependency>
		<dependency>
			<groupId>org.ihc.phc</groupId>
			<artifactId>mh-proxy</artifactId>
			<version>1.0.17</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>buildnumber-maven-plugin</artifactId>
				<version>1.0-beta-4</version>
				<executions>
					<execution>
						<phase>validate</phase>
						<goals>
							<goal>create</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<doCheck>false</doCheck>
					<doUpdate>false</doUpdate>
					<timestampFormat>{0,date,yyyy-MM-dd HH:mm:ss}</timestampFormat>
					<revisionOnScmFailure>DEVELOPER</revisionOnScmFailure>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>build-classpath</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>build-classpath</goal>
						</goals>
						<configuration>
							<outputFile>target/cp.txt</outputFile>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
		<finalName>${project.artifactId}</finalName>
	</build>
</project>
