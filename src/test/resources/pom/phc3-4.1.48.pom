            <exclude>**/TestUsageReport*</exclude>
            <exclude>**/TestUserServiceImpl*</exclude>
            <exclude>**/TestWebProfileDAO3Imple*</exclude>
            <exclude>**/TestWebRegQDaoImpl*</exclude>
            <exclude>**/PhcTestCase.java</exclude>
          </excludes>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
            </manifest>
            <manifestEntries>
              <url>${pom.url}</url>
              <Implementation-Version>4.1.48</Implementation-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-war-plugin</artifactId>
        <version>2.0.2</version>
        <configuration>
          <archive>
            <manifestEntries>
              <Implementation-Version>4.1.48</Implementation-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
    </plugins>
