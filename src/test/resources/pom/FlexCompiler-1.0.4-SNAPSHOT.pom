  <build>
	  <plugins>
	    <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <compilerVersion>1.5</compilerVersion>
          <source>1.5</source>
          <target>1.5</target>
          <debug>true</debug>
          <deprecation>true</deprecation>
          
        </configuration>
      </plugin>      
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <argLine>-Xmx1024m -XX:MaxPermSize=256m</argLine>
          <systemProperties>
            <property><name>PROJECT_HOME</name><value>${basedir}</value></property>
            <property><name>FLEX_HOME</name><value>${env.FLEX_HOME}</value></property>
            <property><name>FLEX_CONFIG</name><value>${env.FLEX_HOME}/frameworks/flex-config-maven.xml</value></property>
            only need this property because of my flex config issues mjm 20090819 -->
          </systemProperties>
        </configuration>
      </plugin>
      	      
	  </plugins>
	</build>
