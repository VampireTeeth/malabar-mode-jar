<?xml version="1.0" encoding="UTF-8"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.ihc.phc.dga</groupId>
  <artifactId>WebMD-DGA</artifactId>
  <name>WebMD-DGA</name>
  <version>1.0.1</version>
  <description>Allow or deny users of My Health access to the WebMD
		functionality</description>
  <url>http://joe/twiki/bin/view/IHC/WebMD</url>
  <ciManagement>
    <system>bamboo</system>
    <url>http://maven.co.ihc.com:9080/bamboo</url>
    <notifiers>
      <notifier>
        <configuration>
          <address>lpmsmith@ihc.com</address>
        </configuration>
      </notifier>
    </notifiers>
  </ciManagement>
  <developers>
    <developer>
      <id>lpmsmith</id>
      <name>Matthew O. Smith</name>
      <email>lpmsmith@ihc.com</email>
    </developer>
  </developers>
  <scm>
    <connection>scm:svn:http://scompile:ps3absta@lp-cmsrc/svn/PersonalHealthConsole/modules/WebMDDGA/trunk</connection>
    <developerConnection>scm:svn:http://scompile:ps3absta@lp-cmsrc/svn/PersonalHealthConsole/modules/WebMDDGA/trunk</developerConnection>
    <url>http://lp-cmsrc/svn/PersonalHealthConsole/modules/WebMDDGA/trunk</url>
  </scm>
  <organization>
    <name>Intermountain Healthcare, Inc.</name>
    <url>http://www.intermountainhealthcare.org</url>
  </organization>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
            </manifest>
            <manifestEntries>
              <Implementation-Version>1.0.1</Implementation-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-source-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-sources</id>
            <phase>package</phase>
            <goals>
              <goal>jar</goal>
              <goal>test-jar</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
        <executions>
          <execution>
            <id>attach-javadocs</id>
            <phase>package</phase>
            <goals>
              <goal>jar</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.codehaus.xfire</groupId>
      <artifactId>xfire-core</artifactId>
      <version>1.2.6</version>
    </dependency>
    <dependency>
      <groupId>org.codehaus.xfire</groupId>
      <artifactId>xfire-aegis</artifactId>
      <version>1.2.6</version>
    </dependency>
    <dependency>
      <groupId>stax</groupId>
      <artifactId>stax-api</artifactId>
      <version>1.0.1</version>
    </dependency>
    <dependency>
      <groupId>org.selecthealth.webmd</groupId>
      <artifactId>SHWebMDServiceClient</artifactId>
      <version>1.1.15</version>
    </dependency>
    <dependency>
      <groupId>org.mockejb</groupId>
      <artifactId>mockejb</artifactId>
      <version>0.6-beta2</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>com.ihc.phc</groupId>
      <artifactId>phc3</artifactId>
      <version>4.1.46</version>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <configuration>
          <instrumentation>
            <ignores>
              <ignore>org.ihc.phc.portlet.emc.ccr.generated.impl.*</ignore>
              <ignore>org.ihc.phc.portlet.emc.ccr.generated.*</ignore>
            </ignores>
          </instrumentation>
        </configuration>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-jxr-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
  <distributionManagement>
    <repository>
      <id>IHC</id>
      <name>IHC</name>
      <url>file:///usr/home/maven</url>
    </repository>
    <site>
      <id>IHC</id>
      <name>IHC</name>
      <url>file:///usr/local/tomcatapps/maven/serv1/webapps/doc/WebMD-DGA</url>
    </site>
    <status>deployed</status>
  </distributionManagement>
</project>