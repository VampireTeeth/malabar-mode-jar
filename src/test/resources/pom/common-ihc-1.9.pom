<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
  <groupId>com.ihc</groupId>
  <artifactId>common-ihc</artifactId>
  <name>Common Code</name>
  <version>1.9</version>
  <description>Common code for use in any project</description>
  <url>http://maven.co.ihc.com/common-ihc/</url>
  <parent>
  	<artifactId>java6-pom</artifactId>
  	<groupId>org.ihc.framework</groupId>
  	<version>1.0.0</version>
  </parent>
  <issueManagement>
    <system>jira</system>
    <url>http://jira.co.ihc.com</url>
  </issueManagement>
  <ciManagement>
    <system>Bamboo</system>
    <url>http://maven.co.ihc.com/bamboo/browse/FRMWRK-COMMON</url>
  </ciManagement>
  <inceptionYear>2006</inceptionYear>
  <developers>
    <developer>
      <id>jkitterm</id>
      <name>Josh Kitterman</name>
      <email>josh.kitterman@intermountainmail.org</email>
    </developer>
    <developer>
      <id>lpmsmith</id>
      <name>Matthew O. Smith</name>
      <email>lpmsmith@ihc.com</email>
      <organization>Enterprise Web Developement</organization>
      <organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
    </developer>
  </developers>
  <scm>
    <connection>scm:svn:https://subversion.co.ihc.com/webdev/general/infrastructure/common/trunk</connection>
    <developerConnection>scm:svn:https://subversion.co.ihc.com/webdev/general/infrastructure/common/trunk</developerConnection>
    <url>https://subversion.co.ihc.com/webdev/general/infrastructure/common/trunk</url>
  </scm>
  <organization>
    <name>Intermountain Healthcare</name>
    <url>http://intermountainhealthcare.org</url>
  </organization>
  <dependencies>
    <dependency>
      <groupId>com.bea</groupId>
      <artifactId>weblogic</artifactId>
      <version>8.1.4.0</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.14</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-context</artifactId>
      <version>3.0.5.RELEASE</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>3.2</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.2</version>
      <scope>test</scope>
    </dependency>
    <dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-context-support</artifactId>
    	<version>3.0.5.RELEASE</version>
    	<type>jar</type>
    	<scope>compile</scope>
    </dependency>
    <dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-jdbc</artifactId>
    	<version>3.0.5.RELEASE</version>
    	<type>jar</type>
    	<scope>compile</scope>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-pmd-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
  <build>
  	<pluginManagement>
  		<plugins>
  			<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
  			<plugin>
  				<groupId>org.eclipse.m2e</groupId>
  				<artifactId>lifecycle-mapping</artifactId>
  				<version>1.0.0</version>
  				<configuration>
  					<lifecycleMappingMetadata>
  						<pluginExecutions>
  							<pluginExecution>
  								<pluginExecutionFilter>
  									<groupId>org.ihc.mojo</groupId>
  									<artifactId>ihc</artifactId>
  									<versionRange>
  										[1.4.1,)
  									</versionRange>
  									<goals>
  										<goal>genprops</goal>
  									</goals>
  								</pluginExecutionFilter>
  								<action>
  									<ignore></ignore>
  								</action>
  							</pluginExecution>
  						</pluginExecutions>
  					</lifecycleMappingMetadata>
  				</configuration>
  			</plugin>
  		</plugins>
  	</pluginManagement>
  </build>
</project>

