        <url>file:///usr/local/maven/repos/ihc</url>
        <uniqueVersion>false</uniqueVersion>
    </snapshotRepository>
    <site>
      <id>ihc-site-deploy</id>
      <name>Intermountain Healthcare Site Deployment Location</name>
      <url>file:///usr/local/maven/webapps/docs</url>
    </site>
  </distributionManagement>
  
	<scm>
		<url>https://subversion.co.ihc.com/webdev/issa/repository/ldap/trunk</url>
		<connection>scm:svn:https://subversion.co.ihc.com/webdev/issa/repository/ldap/trunk/</connection>
		<developerConnection>scm:svn:https://subversion.co.ihc.com/webdev/issa/repository/ldap/trunk/</developerConnection>
