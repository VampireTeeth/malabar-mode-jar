<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.ihc.ewd.mh</groupId>
	<artifactId>mh-clinical-common</artifactId>
	<version>13.12.5</version>
	<packaging>jar</packaging>

	<name>mh-clinical-common</name>
	
	<description>
	This JAR contains clinical related objects that are shared across the various project.  No object in this
	jar should depend upon the CDR-EJB or any web service.
	</description>

	<url>http://maven.co.ihc.com/${project.name}</url>
	<inceptionYear>2010</inceptionYear>

	<parent>
		<groupId>org.ihc.framework</groupId>
		<artifactId>java6-pom</artifactId>
		<version>1.1.5</version>
	</parent>


	<issueManagement>
		<system>Jira</system>
		<url>http://jira.co.ihc.com/browse/PHR</url>
	</issueManagement>
	
	<scm>
		<connection>scm:svn:https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-clinical-common/tags/mh-clinical-common-13.12.5</connection>
		<developerConnection>scm:svn:https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-clinical-common/tags/mh-clinical-common-13.12.5</developerConnection>
		<url>https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-clinical-common/tags/mh-clinical-common-13.12.5</url>
 
	</scm>
	<ciManagement>
  	  <system>Jenkins</system>
  	  <url>http://lpv-webjenkprd01.co.ihc.com/job/mh-clinical-common/</url>
	</ciManagement>
	<developers>
		<developer>
			<id>lpmsmith</id>
			<name>Matthew O. Smith</name>
			<email>lpmsmith@ihc.com</email>
			<url>https://teamspace.intermountain.net/_layouts/userdisp.aspx?ID=129</url>
			<organization>Enterprise Web Development</organization>
			<organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
			<timezone>-7</timezone>
		</developer>
	</developers>

	<build>
		<finalName>${project.name}</finalName>
		<pluginManagement>
			<plugins>
				<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
				<plugin>
					<groupId>org.eclipse.m2e</groupId>
					<artifactId>lifecycle-mapping</artifactId>
					<version>1.0.0</version>
					<configuration>
						<lifecycleMappingMetadata>
							<pluginExecutions>
								<pluginExecution>
									<pluginExecutionFilter>
										<groupId>org.ihc.mojo</groupId>
										<artifactId>ihc</artifactId>
										<versionRange>
											[1.4.1,)
										</versionRange>
										<goals>
											<goal>genprops</goal>
										</goals>
									</pluginExecutionFilter>
									<action>
										<ignore />
									</action>
								</pluginExecution>
							</pluginExecutions>
						</lifecycleMappingMetadata>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.10</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
	</dependencies>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>

</project>
