    <artifactId>framework-core</artifactId>
    <version>1.0.0</version>
    <packaging>jar</packaging>
    <name>EWD Framework Core</name>
    <description>Enterprise Web Development Framework Core</description>
    <url>http://maven.co.ihc.com/framework-core</url>
    <inceptionYear>2008</inceptionYear>
    <scm>
        <url>https://subversion.co.ihc.com/webdev/general/infrastructure/framework/framework-core/trunk</url>
