<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <modelVersion>4.0.0</modelVersion>
    <groupId>org.ihc.imass</groupId>
    <artifactId>imass5-ws-client</artifactId>
    <packaging>jar</packaging>
    <version>5.0.5</version>
    <name>IMASS5 Web Service Client</name>

    <parent>
        <groupId>org.ihc.framework</groupId>
        <artifactId>java5-pom</artifactId>
        <version>1.0.1</version>
    </parent>
    
    
    <build>
    	<plugins>
    		<plugin>
    			<groupId>org.apache.maven.plugins</groupId>
    			<artifactId>maven-compiler-plugin</artifactId>
    			<version>2.3.2</version>
    			<configuration>
    				<source>1.6</source>
    				<target>1.6</target>
    			</configuration>
    		</plugin>
    		<plugin>
	        <groupId>org.codehaus.mojo</groupId>
	        <artifactId>jaxws-maven-plugin</artifactId>
	        <version>1.10</version>
	        <executions>
	          <execution>
	            <goals>
	              <goal>wsimport</goal>
	            </goals>
	          </execution>
	        </executions>
	        <configuration>
	          <packageName>org.ihc.imass5.ws.client</packageName> <!-- The name of your generated source package -->	          
	          <wsdlUrls>
	            <wsdlUrl>${basedir}/app/wsdl/MasterAccountWebService.wsdl</wsdlUrl>
	            <wsdlUrl>${basedir}/app/wsdl/AccountOperationsWebService.wsdl</wsdlUrl>
	          </wsdlUrls>
	          <keep>true</keep>
	          <sourceDestDir>${basedir}/target/src/main/java/</sourceDestDir>
	          <bindingFiles>
	          	<bindingFile>${basedir}/src/main/resources/MakeSerializable.xml</bindingFile>
	          </bindingFiles>
	          <extension>true</extension>
	        </configuration>	        
	        <dependencies>
	          <dependency>
		          <groupId>com.sun.xml.ws</groupId>
		          <artifactId>jaxws-tools</artifactId>
		          <version>2.2.1</version>
	          </dependency>
	        </dependencies>
	      </plugin>    		
    	</plugins>
    </build>
    <properties>
    	<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>
</project>    