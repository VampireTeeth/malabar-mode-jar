<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>org.ihc.mh</groupId>
    <artifactId>mh-regutil</artifactId>
    <version>14.2.1</version>
    <packaging>jar</packaging>
    <url>http://maven.co.ihc.com/mh-regutil</url>

    <inceptionYear>2010</inceptionYear>

    <parent>
        <groupId>org.ihc.framework</groupId>
        <artifactId>java6-pom</artifactId>
        <version>1.1.8</version>
    </parent>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <springVersion>3.0.5.RELEASE</springVersion>
    </properties>

    <scm>
        <url>https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-regutil/tags/mh-regutil-14.2.1</url>
        <connection>scm:svn:https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-regutil/tags/mh-regutil-14.2.1</connection>
        <developerConnection>scm:svn:https://subversion.co.ihc.com/webdev/ewd/intermountainpatientportal/lib/mh-regutil/tags/mh-regutil-14.2.1</developerConnection>
    </scm>
    <ciManagement>
        <system>Jenkins</system>
        <url>http://lpv-webjenkprd01.co.ihc.com/job/mh-regutil/</url>
    </ciManagement>
    <issueManagement>
        <system>Jira</system>
        <url>http://jira.co.ihc.com/browse/PHR</url>
    </issueManagement>
    <developers>
        <developer>
            <id>lpcnucci</id>
            <name>Chris Nuccitelli</name>
            <email>Chris.Nuccitelli@imail.org</email>
            <url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lpcnucci</url>
            <organization>Enterprise Web Development</organization>
            <organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
            <timezone>-7</timezone>
        </developer>
    </developers>
    <build>
        <finalName>${project.artifactId}</finalName>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>buildnumber-maven-plugin</artifactId>
                <version>1.2</version>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>create</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <doCheck>false</doCheck>
                    <doUpdate>false</doUpdate>
                    <timestampFormat>{0,date,yyyy-MM-dd HH:mm:ss}</timestampFormat>
                    <revisionOnScmFailure>DEVELOPER</revisionOnScmFailure>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-dependency-plugin</artifactId>
                <version>2.7</version>
                <executions>
                    <execution>
                        <id>build-classpath</id>
                        <phase>generate-sources</phase>
                        <goals>
                            <goal>build-classpath</goal>
                        </goals>
                        <configuration>
                            <outputFile>target/cp.txt</outputFile>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

    <dependencies>
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>4.11</version>
            <scope>test</scope>
        </dependency>
        <dependency>
            <groupId>org.springframework</groupId>
            <artifactId>spring-context</artifactId>
            <version>${springVersion}</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>javax.validation</groupId>
            <artifactId>validation-api</artifactId>
            <version>1.0.0.GA</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.hibernate</groupId>
            <artifactId>hibernate-validator</artifactId>
            <version>4.3.0.Final</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>org.ihc.service</groupId>
            <artifactId>mh-ldap-service</artifactId>
            <version>1.0.3</version>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>net.sf.beanrunner</groupId>
            <artifactId>beanrunner</artifactId>
            <version>0.0.5</version>
            <scope>test</scope>
            <type>jar</type>
        </dependency>
        <dependency>
            <groupId>javax.portlet</groupId>
            <artifactId>portlet-api</artifactId>
            <version>2.0</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>com.liferay</groupId>
            <artifactId>portal-service</artifactId>
            <version>6.0.6</version>
            <type>jar</type>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.ihc.phc</groupId>
            <artifactId>mh-proxy</artifactId>
            <version>14.5.1</version>
            <type>jar</type>
            <exclusions>
                <exclusion>
                    <artifactId>log4j</artifactId>
                    <groupId>log4j</groupId>
                </exclusion>
                <exclusion>
                    <artifactId>slf4j-log4j12</artifactId>
                    <groupId>org.slf4j</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <groupId>org.ihc.audit</groupId>
            <artifactId>AuditEJB</artifactId>
            <version>3.2.0</version>
            <type>jar</type>
            <classifier>glassfishclient</classifier>
            <scope>compile</scope>
            <exclusions>
                <exclusion>
                    <artifactId>log4j</artifactId>
                    <groupId>log4j</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <artifactId>spring-jdbc</artifactId>
            <groupId>org.springframework</groupId>
            <type>jar</type>
            <version>${springVersion}</version>
        </dependency>
        <dependency>
            <groupId>joda-time</groupId>
            <artifactId>joda-time</artifactId>
            <version>2.1</version>
        </dependency>
    </dependencies>
    <name>My Health Registration Shared Libraries</name>
</project>
