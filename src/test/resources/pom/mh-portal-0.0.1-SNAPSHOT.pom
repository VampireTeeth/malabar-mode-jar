<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	
	<parent>
		<groupId>org.ihc.framework</groupId>
		<artifactId>java5-pom</artifactId>
		<version>1.0.1</version>
	</parent>

	<groupId>org.ihc.mh</groupId>
	<artifactId>mh-portal</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<packaging>jar</packaging>

	<name>mh-portal</name>
	<url>http://maven.co.ihc.com/mh-portal/</url>
	<description>
	   A library of portal specific utilities that will need to be ported when the underlying portal changes.
	   Any portal-specific code goes in this jar.
	</description>
	<issueManagement>
		<system>Jira</system>
		<url>http://jira.co.ihc.com/browse/PHR</url>
	</issueManagement>
	<ciManagement>
		<system>Bamboo</system>
		<url>http://maven.co.ihc.com/bamboo/browse/MH-PORTAL</url>
	</ciManagement>
	<inceptionYear>2011</inceptionYear>
	<scm>
		<url>https://subversion.co.ihc.com/webdev/general/service/MyHealth/modules/mh-portal/trunk</url>
		<connection>scm:svn:https://subversion.co.ihc.com/webdev/general/service/MyHealth/modules/mh-portal/trunk</connection>
		<developerConnection>scm:svn:https://subversion.co.ihc.com/webdev/general/service/MyHealth/modules/mh-portal/trunk</developerConnection>
	</scm>
	<developers>
		<developer>
			<id>lpmsmith</id>
			<name>Matthew Smith</name>
			<email>Matthew.Smith@imail.org</email>
			<url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lpmsmith</url>
			<organization>Enterprise Web Development</organization>
			<organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
			<timezone>-7</timezone>
		</developer>
	</developers>
	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>
	<build>
		<finalName>${project.artifactId}</finalName>
		<plugins>
			<plugin>
				<groupId>org.codehaus.mojo</groupId>
				<artifactId>buildnumber-maven-plugin</artifactId>
				<version>1.0-beta-4</version>
				<executions>
					<execution>
						<phase>validate</phase>
						<goals>
							<goal>create</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<doCheck>false</doCheck>
					<doUpdate>false</doUpdate>
					<timestampFormat>{0,date,yyyy-MM-dd HH:mm:ss}</timestampFormat>
					<revisionOnScmFailure>DEVELOPER</revisionOnScmFailure>
				</configuration>
			</plugin>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<executions>
					<execution>
						<id>build-classpath</id>
						<phase>generate-sources</phase>
						<goals>
							<goal>build-classpath</goal>
						</goals>
						<configuration>
							<outputFile>target/cp.txt</outputFile>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>org.springframework</groupId>
			<artifactId>spring-context</artifactId>
			<version>3.0.5.RELEASE</version>
			<type>jar</type>
			<scope>compile</scope>
		</dependency>
		<dependency>
			<groupId>javax.portlet</groupId>
			<artifactId>portlet-api</artifactId>
			<version>2.0</version>
			<type>jar</type>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>net.sf.portletunit</groupId>
			<artifactId>portletunit2</artifactId>
			<version>1.0.3-SNAPSHOT</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.8.2</version>
			<type>jar</type>
			<scope>test</scope>
		</dependency>
	</dependencies>
</project>
