<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  
  <parent>
    <groupId>org.ihc.framework</groupId>
    <artifactId>java5-pom</artifactId>
    <version>1.0.1</version>
  </parent>
  
  <groupId>com.ihc</groupId>
  <artifactId>gad</artifactId>
  <version>1.8.22</version>
  <name>GAD</name>
  <description>General Data Access library for interacting with medical records
  	contained in the CDR</description>
  <ciManagement>
    <system>bamboo</system>
    <url>http://maven.co.ihc.com/bamboo/browse/PHC-GAD</url>
    <notifiers>
      <notifier>
        <configuration>
          <address>Matthew.Smith@imail.org</address>
        </configuration>
      </notifier>
    </notifiers>
  </ciManagement>
  <url>http://maven.co.ihc.com/gad/</url>
  <issueManagement>
	<system>jira</system>
	<url>http://jira.co.ihc.com/browse/PHC</url>
  </issueManagement>
  <organization>
	<name>Intermountian Healthcare, Inc.</name>
	<url>http://intermountainhealthcare.org</url>
  </organization>
  <scm>
	<connection>
	  scm:svn:https://scompile:ps3absta@subversion.co.ihc.com/webdev/general/service/GAD/
	</connection>
	<developerConnection>
	  scm:svn:https://scompile:ps3absta@subversion.co.ihc.com/webdev/general/service/GAD/
	</developerConnection>
	<url>https://subversion.co.ihc.com/webdev/general/service/GAD/</url>
  </scm>
  <developers>
	<developer>
	  <id>lpmsmith</id>
	  <name>Matthew O. Smith</name>
	  <email>lpmsmith@ihc.com</email>
	  <url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lpmsmith</url>
	  <organization>Enterprise Web Development</organization>
	  <organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
	  <timezone>-7</timezone>
	</developer>
	<developer>
	  <id>lpcnucci</id>
	  <name>Chris Nuccitelli</name>
	  <email>lpcnucci@ihc.com</email>
	  <url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lpcnucci</url>
	  <organization>Enterprise Web Development</organization>
	  <organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
	  <timezone>-7</timezone>
	</developer>
	<developer>
	  <id>lphpost</id>
	  <name>Herman Post</name>
	  <email>lphpost@ihc.com</email>
	  <url>http://prtlmy.intermountain.net/Person.aspx?accountname=CO\lphpost</url>
	  <organization>Enterprise Web Development</organization>
	  <organizationUrl>https://teamspace.intermountain.net/ewd</organizationUrl>
	  <timezone>-7</timezone>
	</developer>
  </developers>
  
  <build>
  	<finalName>gad</finalName>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-jar-plugin</artifactId>
          <version>2.1</version>
        </plugin>
      </plugins>
    </pluginManagement>
    <plugins>
      <plugin>
        <artifactId>maven-surefire-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-jar-plugin</artifactId>
        <configuration>
          <archive>
            <manifest>
              <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
              <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
            </manifest>
            <manifestEntries>
              <url>${pom.url}</url>
              <Implementation-Version>${project.version}</Implementation-Version>
            </manifestEntries>
          </archive>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>commons-collections</groupId>
      <artifactId>commons-collections</artifactId>
      <version>3.2</version>
    </dependency>
    <dependency>
      <groupId>commons-lang</groupId>
      <artifactId>commons-lang</artifactId>
      <version>2.6</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>log4j</groupId>
      <artifactId>log4j</artifactId>
      <version>1.2.8</version>
    </dependency>
    <dependency>
      <groupId>org.ihc.cdr</groupId>
      <artifactId>cdr-ejb</artifactId>
      <version>2.14.69</version>
    </dependency>
	<dependency>
	  <groupId>com.ihc</groupId>
	  <artifactId>common-ihc-vgn-log</artifactId>
	  <version>1.2.1</version>
	</dependency>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-core</artifactId>
      <version>3.0.4.RELEASE</version>
    </dependency>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-test</artifactId>
      <version>3.0.4.RELEASE</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>      
    </dependency>
    <dependency>
      <groupId>com.ihc</groupId>
      <artifactId>common-tools-ihc</artifactId>
      <version>1.0.58</version>
      <scope>test</scope>      
    </dependency>
    <dependency>
    	<groupId>javax.servlet</groupId>
    	<artifactId>servlet-api</artifactId>
    	<version>2.3</version>
    	<type>jar</type>
    	<scope>provided</scope>
    </dependency>
    <dependency>
    	<groupId>com.bea</groupId>
    	<artifactId>weblogic</artifactId>
    	<version>8.1.4.0</version>
    	<type>jar</type>
    	<scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.bigfaceless</groupId>
      <artifactId>bforeport</artifactId>
      <version>1.1.46</version>
    </dependency>
    
    <dependency>
    	<groupId>net.sf.beanrunner</groupId>
    	<artifactId>beanrunner</artifactId>
    	<version>0.0.3</version>
    	<type>jar</type>
    	<scope>test</scope>
    </dependency>
    <dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-context</artifactId>
    	<version>3.0.4.RELEASE</version>
    	<type>jar</type>
    	<scope>compile</scope>
    </dependency>
    <dependency>
    	<groupId>net.sf.jtidy</groupId>
    	<artifactId>jtidy</artifactId>
    	<version>r938</version>
    	<type>jar</type>
    	<scope>compile</scope>
    </dependency>
    <dependency>
    	<groupId>org.springframework</groupId>
    	<artifactId>spring-web</artifactId>
    	<version>3.0.5.RELEASE</version>
    	<type>jar</type>
    	<scope>compile</scope>
    </dependency>
  </dependencies>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-javadoc-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-surefire-report-plugin</artifactId>
      </plugin>
      <plugin>
        <artifactId>maven-jxr-plugin</artifactId>
      </plugin>
      <plugin>
      	<groupId>org.apache.maven.plugins</groupId>
      	<artifactId>maven-dependency-plugin</artifactId>
      	<reportSets>
      		<reportSet>
      			<reports>
      				<report>analyze-report</report>
      			</reports>
      		</reportSet>
      	</reportSets>
      </plugin>
    </plugins>
  </reporting>
  
</project>
